  <!-- исправление согласно концепции «12+» -->

  <!-- EN: tainted {0} -->
  <DeadMansApparel.stages.tainted.label>{0} в чужой крови</DeadMansApparel.stages.tainted.label>
  <!-- EN: I am wearing a piece of apparel that someone died in. It creeps me out and feels dirty. -->
