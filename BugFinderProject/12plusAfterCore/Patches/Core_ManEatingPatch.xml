<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!-- Исправление согласно концепции «12+» -->

	<Operation Class="PatchOperationAdd">
		<success>Always</success>
		<xpath>Defs/ThingDef[@Name = "BasePawn"]</xpath>
		<value>
			<race>
				<willNeverEat>
					<li>Meat_Human</li>
					<li>Corpse_Human</li>
					<!-- Заодно убираем съедобность ключевых трупов из других модов, почему-то отдельным патчем она не отключается, вероятно из-за алфавитного порядка файлов патчей -->
					<li MayRequire="dianawinters.OrassansLite">Corpse_Alien_Orassan</li>
					<li MayRequire="dianawinters.OrassansLite">Corpse_Alien_OrassanHumanHybrid</li>
					<li MayRequire="Atlas.AndroidTiers">Corpse_Abomination</li>
					<li MayRequire="Atlas.AndroidTiers">Corpse_MicroScyther</li>
					<li MayRequire="sarg.alphaanimals">Corpse_AA_FungalHusk</li>
					<li MayRequire="sarg.alphaanimals">Corpse_AA_Mime</li>

				</willNeverEat>
			</race>
		</value>
	</Operation>

</Patch>
